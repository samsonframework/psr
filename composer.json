{
	"name":"samsonframework/psr",
    "description": "SamsonFramework PHPFIG interfaces",
    "keywords": ["SamsonFramework", "PHPFIG", "interfaces"],
    "authors":[
        {
            "name":"Vitaly Iegorov",
            "email":"egorov@samsonos.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "4.2.*"
    },
    "minimum-stability":"dev",
	"license":"Open Software License (OSL) v 3.0",
